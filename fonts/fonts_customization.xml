<fonts-modification version="1">
    <family customizationType="new-named-family" name="g-sans">
        <font weight="100" style="normal">GoogleSans-Thin.ttf</font>
        <font weight="100" style="italic">GoogleSans-ThinItalic.ttf</font>
        <font weight="300" style="normal">GoogleSans-Light.ttf</font>
        <font weight="300" style="italic">GoogleSans-LightItalic.ttf</font>
        <font weight="400" style="normal">GoogleSans-Regular.ttf</font>
        <font weight="400" style="italic">GoogleSans-Italic.ttf</font>
        <font weight="500" style="normal">GoogleSans-Medium.ttf</font>
        <font weight="500" style="italic">GoogleSans-MediumItalic.ttf</font>
        <font weight="700" style="normal">GoogleSans-Bold.ttf</font>
        <font weight="700" style="italic">GoogleSans-BoldItalic.ttf</font>
        <font weight="900" style="normal">GoogleSans-Black.ttf</font>
        <font weight="900" style="italic">GoogleSans-BlackItalic.ttf</font>
    </family>
    <alias name="g-sans-medium" to="g-sans" weight="500"/>

    <family customizationType="new-named-family" name="Chirp">
        <font weight="200" style="normal">Chirp.ttf</font>
    </family>
    <alias name="Chirp-medium" to="Chirp" weight="500" />
    <alias name="Chirp-semi-bold" to="Chirp" weight="600" />
    <alias name="Chirp-bold" to="Chirp" weight="700" />

    <family customizationType="new-named-family" name="Circe">
        <font weight="200" style="normal">Circe.ttf</font>
    </family>
    <alias name="Circe-medium" to="Circe" weight="500" />
    <alias name="Circe-semi-bold" to="Circe" weight="600" />
    <alias name="Circe-bold" to="Circe" weight="700" />

    <family customizationType="new-named-family" name="Euclid">
        <font weight="200" style="normal">Euclid.ttf</font>
    </family>
    <alias name="Euclid-medium" to="Euclid" weight="500" />
    <alias name="Euclid-semi-bold" to="Euclid" weight="600" />
    <alias name="Euclid-bold" to="Euclid" weight="700" />

    <family customizationType="new-named-family" name="Fluid">
        <font weight="200" style="normal">Fluid.ttf</font>
    </family>
    <alias name="Fluid-medium" to="Fluid" weight="500" />
    <alias name="Fluid-semi-bold" to="Fluid" weight="600" />
    <alias name="Fluid-bold" to="Fluid" weight="700" />

    <family customizationType="new-named-family" name="manrope">
    <font weight="500" style="normal">Manrope-VF.ttf
        <axis tag="wght" stylevalue="500"/>
    </font>
    <font weight="600" style="normal">Manrope-VF.ttf
        <axis tag="wght" stylevalue="600"/>
    </font>
    <font weight="700" style="normal">Manrope-VF.ttf
        <axis tag="wght" stylevalue="700"/>
    </font>
    <font weight="800" style="normal">Manrope-VF.ttf
        <axis tag="wght" stylevalue="800"/>
    </font>
    </family>
    <alias name="manrope-medium" to="manrope" weight="600" />

    <family customizationType="new-named-family" name="inter">
    <font weight="100" style="normal">Inter-VF.ttf
        <axis tag="wght" stylevalue="100"/>
    </font>
    <font weight="100" style="italic">Inter-VF.ttf
        <axis tag="wght" stylevalue="100"/>
    </font>
    <font weight="200" style="normal">Inter-VF.ttf
        <axis tag="wght" stylevalue="200"/>
    </font>
    <font weight="200" style="italic">Inter-VF.ttf
        <axis tag="wght" stylevalue="200"/>
    </font>
    <font weight="300" style="normal">Inter-VF.ttf
        <axis tag="wght" stylevalue="300"/>
    </font>
    <font weight="300" style="italic">Inter-VF.ttf
        <axis tag="wght" stylevalue="300"/>
    </font>
    <font weight="400" style="normal">Inter-VF.ttf
        <axis tag="wght" stylevalue="400"/>
    </font>
    <font weight="400" style="italic">Inter-VF.ttf
        <axis tag="wght" stylevalue="400"/>
    </font>
    <font weight="500" style="normal">Inter-VF.ttf
        <axis tag="wght" stylevalue="500"/>
    </font>
    <font weight="500" style="italic">Inter-VF.ttf
        <axis tag="wght" stylevalue="500"/>
    </font>
    <font weight="700" style="normal">Inter-VF.ttf
        <axis tag="wght" stylevalue="700"/>
    </font>
    <font weight="700" style="italic">Inter-VF.ttf
        <axis tag="wght" stylevalue="700"/>
    </font>
    <font weight="900" style="normal">Inter-VF.ttf
        <axis tag="wght" stylevalue="900"/>
    </font>
    <font weight="900" style="italic">Inter-VF.ttf
        <axis tag="wght" stylevalue="900"/>
    </font>
    </family>
    <alias name="inter-medium" to="inter" weight="500" />

    <family customizationType="new-named-family" name="linotte">
    <font weight="300" style="normal">Linotte.ttf</font>
    </family>
    <alias name="linotte-semi-bold" to="linotte" weight="600" />
    <alias name="linotte-bold" to="linotte" weight="700" />

    <family customizationType="new-named-family" name="oneplusslate">
    <font weight="300" style="normal">SlateFromOP-Light.ttf</font>
    <font weight="400" style="normal">SlateFromOP-Regular.ttf</font>
    </family>
    <alias name="oneplusslate-bold" to="oneplusslate" weight="600" />

    <family customizationType="new-named-family" name="op-sans">
        <font weight="100" style="normal">OnePlusSans-Thin.ttf</font>
        <font weight="100" style="italic">OnePlusSans-ThinItalic.ttf</font>
        <font weight="300" style="normal">OnePlusSans-Light.ttf</font>
        <font weight="300" style="italic">OnePlusSans-LightItalic.ttf</font>
        <font weight="400" style="normal">OnePlusSans-Regular.ttf</font>
        <font weight="400" style="italic">OnePlusSans-Italic.ttf</font>
        <font weight="500" style="normal">OnePlusSans-Medium.ttf</font>
        <font weight="500" style="italic">OnePlusSans-MediumItalic.ttf</font>
        <font weight="900" style="normal">OnePlusSans-Black.ttf</font>
        <font weight="900" style="italic">OnePlusSans-BlackItalic.ttf</font>
        <font weight="700" style="normal">OnePlusSans-Bold.ttf</font>
        <font weight="700" style="italic">OnePlusSans-BoldItalic.ttf</font>
    </family>
    <alias name="op-sans-thin" to="op-sans" weight="100" />
    <alias name="op-sans-light" to="op-sans" weight="300" />
    <alias name="op-sans-book" to="op-sans" weight="400" />
    <alias name="op-sans-regular" to="op-sans" weight="400" />
    <alias name="op-sans-medium" to="op-sans" weight="500" />
    <alias name="op-sans-bold" to="op-sans" weight="700" />
    <alias name="op-sans-black" to="op-sans" weight="900" />

    <family customizationType="new-named-family" name="harmonyos-sans">
        <font weight="100" style="normal">HarmonyOS-Sans-Thin.ttf</font>
        <font weight="100" style="italic">HarmonyOS-Sans-ThinItalic.ttf</font>
        <font weight="300" style="normal">HarmonyOS-Sans-Light.ttf</font>
        <font weight="300" style="italic">HarmonyOS-Sans-LightItalic.ttf</font>
        <font weight="400" style="normal">HarmonyOS-Sans-Regular.ttf</font>
        <font weight="400" style="italic">HarmonyOS-Sans-Italic.ttf</font>
        <font weight="500" style="normal">HarmonyOS-Sans-Medium.ttf</font>
        <font weight="500" style="italic">HarmonyOS-Sans-MediumItalic.ttf</font>
        <font weight="700" style="normal">HarmonyOS-Sans-Bold.ttf</font>
        <font weight="700" style="italic">HarmonyOS-Sans-BoldItalic.ttf</font>
        <font weight="900" style="normal">HarmonyOS-Sans-Black.ttf</font>
        <font weight="900" style="italic">HarmonyOS-Sans-BlackItalic.ttf</font>
    </family>
    <alias name="harmonyos-sans-medium" to="harmonyos-sans" weight="500"/>

    <family customizationType="new-named-family" name="SFPro">
        <font weight="200" style="normal">SFPro.ttf</font>
    </family>
    <alias name="SFPro-medium" to="SFPro" weight="500" />
    <alias name="SFPro-semi-bold" to="SFPro" weight="600" />
    <alias name="SFPro-bold" to="SFPro" weight="700" />

    <family customizationType="new-named-family" name="urbanist">
    <font weight="500" style="normal">Urbanist-VF.ttf
        <axis tag="wght" stylevalue="500"/>
    </font>
    <font weight="600" style="normal">Urbanist-VF.ttf
        <axis tag="wght" stylevalue="600"/>
    </font>
    <font weight="700" style="normal">Urbanist-VF.ttf
        <axis tag="wght" stylevalue="700"/>
    </font>
    <font weight="800" style="normal">Urbanist-VF.ttf
        <axis tag="wght" stylevalue="800"/>
    </font>
    </family>
    <alias name="urbanist-medium" to="urbanist" weight="600" />
</fonts-modification>
